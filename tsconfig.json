{
	"compileOnSave": false,
	"compilerOptions": {
		"allowJs": false,
		"forceConsistentCasingInFileNames": true,
		"listFiles": true,
		"module": "commonjs",
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"pretty": true,
		"sourceMap": true,
		"target": "es5",
		"outDir": "dist",
		"version": true
	}
}
